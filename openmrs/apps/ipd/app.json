                    },
                    "expandedViewConfig": {
                        "numberOfEncounters": 6,
                        "conceptNames": [
                            "AAPOSTOP, Date recorded",
                            "AAPOSTOP, Anesthesia technique",
                            "AAPOSTOP, Mobilization date/time",
