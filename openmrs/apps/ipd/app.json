                "surgicalAppointments": {
                    "type": "custom",
                    "displayOrder": 3,
                    "config": {
                        "title": "DASHBOARD_TITLE_SURGICAL_APPOINTMENTS_KEY",
                        "template": "<surgical-appointments section='section' patient='patient'></surgical-appointments>"
                    }
                },
                "IPD Assessment": {
                    "translationKey": "IPD_ASSESSMENT_KEY",
